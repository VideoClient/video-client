{
  "name": "video-client",
  "version": "0.1.0",
  "description": "一款PC端视频聚合应用，提供国内主流视频站资源。",
  "main": "app/main.js",
  "scripts": {
    "start": "electron-forge start",
    "install": "typings install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "package-all": "electron-forge package -a x64 -p all",
    "make": "electron-forge make"
  },
  "keywords": [
    "video",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/npm.git"
  },
  "url": "https://github.com/owner/project/issues",
  "email": "sunxfancy@gmail.com",
  "author": {
    "name": "sunxfancy",
    "email": "sunxfancy@gmail.com",
    "url": "https://github.com/sunxfancy"
  },
  "license": "MIT",
  "devDependencies": {
    "devtron": "^1.3.0",
    "electron-forge": "^2.7.2",
    "electron-prebuilt-compile": "1.4.15",
    "electron-react-devtools": "^0.4.0",
    "typescript": "^2.1.5",
    "typings": "^2.1.0"
  },
  "dependencies": {
    "electron-compile": "^5.1.3",
    "electron-gh-releases": "^2.0.4",
    "github-download": "^0.5.0",
    "material-ui": "^0.16.7",
    "nedb": "^1.8.0",
    "npm": "^4.1.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-html5video": "^1.4.0",
    "react-layout-components": "^3.0.1",
    "react-materialui-notifications": "^0.4.1",
    "react-router": "^3.0.2",
    "react-swipeable-views": "^0.10.3",
    "react-tap-event-plugin": "^2.0.1",
    "request": "^2.79.0",
    "youku-client": "^0.1.2"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "zip",
          "squirrel",
          "appx"
        ],
        "darwin": [
          "zip",
          "dmg"
        ],
        "linux": [
          "zip",
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "ignore": [
          ".gitignore",
          "typings",
          "tsconfig.json",
          "typings.json"
        ]
      },
      "electronWinstallerConfig": {
        "iconUrl": "app/icon/icon.ico",
        "setupIcon": "app/icon/icon.ico"
      },
      "electronInstallerDebian": {
        "icon": "app/icon/icon.png",
        "categories": [
          "Video"
        ]
      },
      "electronInstallerRedhat": {
        "icon": "app/icon/icon.png",
        "categories": [
          "Video"
        ]
      },
      "electronInstallerDMG": {
        "icon": "app/icon/icon.ico",
        "icon-size": 64
      },
      "github_repository": {
        "owner": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}
